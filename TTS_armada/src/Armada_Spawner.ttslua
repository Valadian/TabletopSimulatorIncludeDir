function PurgeNGrams(o)
    if o~=nil then
        for key,ngram in pairs(NGRAMS) do
            for i,obj in ipairs(ngram) do
                if obj==o then
                    NGRAMS[key][i] = nil
                end
            end
        end
    end
end
